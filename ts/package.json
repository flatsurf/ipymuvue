{
  "name": "ipyvue3implementation",
  "version": "0.0.1",
  "description": "Reactive Jupyter Widgets",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:es:dev": "cross-env NODE_ENV=development rollup --config build/rollup.config.js --format es",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "main": "dist/ipyvue3implementation.ssr.js",
  "module": "dist/ipyvue3implementation.esm.js",
  "browser": "dist/ipyvue3implementation.esm.js",
  "unpkg": "dist/ipyvue3implementation.min.js",
  "files": [
    "dist/*",
    "ipyvue3implementation.d.ts",
    "src/**/*.vue"
  ],
  "dependencies": {
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "@babel/preset-typescript": "^7.18.6",
    "@jupyter-widgets/base": "^1.1 || ^2 || ^3 || ^4",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-typescript": "^8.3.3",
    "@types/lodash-es": "^4.17.6",
    "@vue/compiler-sfc": "^3.2.37",
    "cross-env": "^7.0.3",
    "crypto": "1.0.1",
    "minimist": "^1.2.6",
    "rollup": "^2.77.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "vue": "^3.2.37"
  },
  "peerDependencies": {
    "@jupyter-widgets/base": "^1.1 || ^2 || ^3 || ^4",
    "vue": "^3.2.37"
  },
  "engines": {
    "node": ">=10"
  },
  "types": "ipyvue3implementation.d.ts"
}
